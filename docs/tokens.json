{
  "$name": "Cobalt",
  "color": {
    "$type": "color",
    "black":     { "$value": "#00193f" },
    "blue": {
      "03": { "$value": "#1a2354" },
      "04": { "$value": "#001585" },
      "05": { "$value": "#2b3faa" },
      "06": { "$value": "#1346bd" }
    },
    "darkGray":  { "$value": "#282a37" },
    "green":     { "$value": "#77cd8f" },
    "purple":    { "$value": "#6a35d9" },
    "red":       { "$value": "#f6566a" },
    "white":     { "$value": "#ffffff" },
    "code": {
      "bg": {
        "greenShift": { "$value": "#2b5eaa" }
      },
      "gold":    { "$value": "#ffdc2b" },
      "green":   { "$value": "#79ff6a" },
      "orange":  { "$value": "#ff6418" },
      "red":     { "$value": "#f56d65" },
      "pink":    { "$value": "#ff5ae2" },
      "skyBlue": { "$value": "#59c1ff" }
    }
  },
  "font": {
    "family": {
      "$type": "font",
      "ppNeue": { "$value": ["PP Neue Montreal", "system-ui", "ui-sans-serif", "-apple-system", "Helvetica Neue", "Helvetica", "Arial", "sans-serif"] },
      "mono":   { "$value": ["Red Hat Mono", "ui-monospace", "SF Mono", "Monaco", "Cascadia Mono", "Cascadia Code", "Consolas", "monospace"] }
    },
    "size": {
      "$type": "dimension",
      "01": { "$value": "8px" },
      "02": { "$value": "9px" },
      "03": { "$value": "12px" },
      "04": { "$value": "14px" },
      "05": { "$value": "18px" },
      "06": { "$value": "22px" },
      "07": { "$value": "28px" },
      "08": { "$value": "32px" }
    }
  },
  "space": {
    "$description": "IBM Carbon spacing",
    "$type": "dimension",
    "4xs": { "$value": "1px" },
    "3xs": { "$value": "2px" },
    "2xs": { "$value": "4px" },
    "xs": { "$value": "8px" },
    "sm": { "$value": "12px" },
    "md": { "$value": "16px" },
    "lg": { "$value": "24px" },
    "xl": { "$value": "32px" },
    "2xl": { "$value": "40px" },
    "3xl": { "$value": "48px" },
    "layout": {
      "2xs": { "$value": "16px" },
      "xs": { "$value": "24px" },
      "sm": { "$value": "32px" },
      "md": { "$value": "48px" },
      "lg": { "$value": "64px" },
      "xl": { "$value": "96px" },
      "2xl": { "$value": "160px" }
    }
  },
  "easing": {
    "$type": "cubicBezier",
    "linear": { "$value": [0, 0, 1, 1] },
    "easeOutCubic": { "$value": [0.33, 1, 0.68, 1] },
    "easeInCubic": { "$value": [0.32, 0, 0.67, 1] },
    "easeInOutCubic": { "$value": [0.65, 0, 0.35, 1] }
  }
}
