{
  "color": {
    "$type": "color",
    "gray": {
      "$value": "#544741",
      "$extensions": {
        "mode": {"light": "#544741", "dark": "#bbb6b3"}
      }
    }
  },
  "border": {
    "$type": "border",
    "$value": {
      "color": "#0d0300",
      "width": "1px",
      "style": "solid"
    },
    "$extensions": {
      "mode": {
        "light": {
          "color": "#0d0300",
          "width": "1px",
          "style": "solid"
        },
        "dark": {
          "color": "#ffffff",
          "width": "1px",
          "style": "solid"
        }
      }
    }
  },
  "border-gray": {
    "$type": "border",
    "$value": {
      "color": "{color.gray}",
      "width": "1px",
      "style": "solid"
    }
  }
}
